#include <stdio.h>
#include <tchar.h>
#include <iostream>
#include <cstdlib>
#include <string>
#include <malloc.h>
#include <fstream>
#include "Source.h"
using namespace std;
void Exception(string name)
{
	fstream file(name);
	if
		(!file.is_open()) cout << "Not open\n"; // если не открылся
	else if
		(file.peek() == EOF) cout << "File empty\n"; // если первый символ конец файла
}
int main()
{

	string name = "text.txt";
	{
		ifstream file;
		file.open(name);
		Exception(name);
		while (!file.eof())
		{
			string string;
			getline(file, string);
			if (doublenumber(string) == true)
			{
				cout << string;
			}
		}

		file.close();
	}
	return 0;
}
